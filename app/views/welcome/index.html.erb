<h1>Welcome!</h1>

<% if current_user %>
  <p>Hello, <%= current_user.name %></p>

  <p>
  <%= form_for :tweet, url: tweets_path, method: :post do |f| %>
    <%= f.text_field :message %>
    <%= f.submit "Tweet!" %>
  <% end %>
</p>

 <% @twitter_data.client.user_timeline.each do |tweet| %>
   <%= tweet.text %>
  <% end %>
<%= link_to "Logout", logout_path %>

<%= link_to "Login", login_path %>
  <% end %>
